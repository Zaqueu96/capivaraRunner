{
  "name": "capivararunner",
  "displayName": "CapivaraRunner",
  "description": "For start many services required ",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:capivaraTreeViewActBar"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "capivaraTreeView",
          "title": "Capivara Runner",
          "icon": "media/capivara_white.svg"
        }
      ]
    },
    "views": {
      "capivaraTreeView": [
        {
          "id": "capivaraTreeView",
          "name": "Services"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.startAllServices",
        "title": "StartAll",
        "icon": {
          "light": "media/start.png",
          "dark": "media/start_white.png"
        }
      },
      {
        "command": "extension.stopAllServices",
        "title": "StopAll",
        "icon": {
          "light": "media/stop.png",
          "dark": "media/stop_white.png"
        }
      },
      {
        "command": "extension.refresh",
        "title": "Refresh",
        "icon": {
          "light": "media/refresh.png",
          "dark": "media/refresh_white.png"
        }
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "extension.startAllServices",
          "when": "view == capivaraTreeView",
          "group": "navigation"
        },
        {
          "command": "extension.stopAllServices",
          "when": "view == capivaraTreeView",
          "group": "navigation"
        },
        {
          "command": "extension.refresh",
          "when": "view == capivaraTreeView",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./src/main",
    "watch": "tsc -watch -p ./src/main",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "20.x",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.9.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.4",
    "vscode-test": "^1.6.1"
  },
  "dependencies": {
    "terminate": "^2.8.0"
  }
}
